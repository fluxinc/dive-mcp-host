{
    "mcpServers": {
        "rag-mcp-server": {
            "transport": "command",
            "command": "node",
            "enabled": true,
            "args": [
                "/app/RAG-mcp-server/dist/index.js",
                "--log"
            ],
            "env": {
                "LOG_FILE_PATH": "/app/RAG-mcp-server/logs/mcp-server.log",
                "DATABRIDGE_URL": "http://morphik:8000"
            }
        }
    }
}
